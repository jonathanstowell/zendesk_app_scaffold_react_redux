{
  "name": "app_scaffold",
  "version": "1.0.1",
  "description": "A scaffold for developers to build ZAF v2 apps",
  "keywords": [
    "zendesk",
    "app",
    "framework"
  ],
  "author": "Zendesk",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.11.5"
  },
  "scripts": {
    "set:dev": "set 'NODE_ENV=development' && set 'BABEL_ENV=development'",
    "set:prod": "set 'NODE_ENV=production' && set 'BABEL_ENV=production'",
    "test": "node run-jest.js --env=jsdom",
    "watch": "npm run set:dev && webpack --watch --mode=development",
    "build:dev": "npm run set:dev && webpack --mode=development",
    "build": "npm run set:prod && webpack -p --mode=production",
    "lint": "standard",
    "start": "zat server -p ./dist --unattended",
    "start:dev": "npm run set:dev && webpack-dev-server",
    "publish": "node run-jest.js --env=jsdom --coverage && npm run build && zat validate --path=./dist && zat package --path=./dist"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
