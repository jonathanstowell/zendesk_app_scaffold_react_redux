    "@babel/preset-stage-2": "7.0.0",
    "@zendeskgarden/css-arrows": "^3.1.1",
    "@zendeskgarden/css-avatars": "^3.0.3",
    "@zendeskgarden/css-bedrock": "^7.0.3",
    "@zendeskgarden/css-buttons": "^6.0.3",
    "@zendeskgarden/css-callouts": "^3.0.3",
    "@zendeskgarden/css-forms": "^6.0.5",
    "@zendeskgarden/css-grid": "^0.1.6",
    "@zendeskgarden/css-menus": "^7.0.4",
    "@zendeskgarden/css-modals": "^6.1.2",
    "@zendeskgarden/css-pagination": "^3.0.3",
    "@zendeskgarden/css-tables": "^3.0.5",
    "@zendeskgarden/css-tabs": "^5.0.3",
    "@zendeskgarden/css-tags": "^4.0.4",
    "@zendeskgarden/css-tooltips": "^4.0.4",
    "@zendeskgarden/css-utilities": "^3.0.1",
    "babel-core": "^6.26.3",
